@page "/cidades/deletar"


<h4>Deletar Cidades</h4>


<div class="row">
    <div class="col-md-6 col-sm-12">
        <div class="col-md-3 col-sm-12">
            <label>Id</label>
            <input type="text" name="Id" class="form-control">
        </div>
    </div>
</div>

<div class="row">
    <button onclick="window.location.href='/cidades'">Cancelar</button>
    <button onclick="deletar();">Deletar</button>
</div>

@section Scripts {
    <script src="~/js/pages/cidades-delete.js"></script>
}